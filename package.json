{
  "name": "electron-music",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "hzihhuang",
  "scripts": {
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "start:web": "vite preview",
    "dev:web": "vite dev",
    "build": "vue-tsc && vite build",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "preview": "vite preview",
    "eslint": "eslint . --ext .vue,.js,.ts",
    "eslint:fix": "eslint . --ext .vue,.js,.ts --fix",
    "prepare": "husky install",
    "stylelint": "stylelint \"**/*.{vue,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "stylelint:fix": "stylelint --fix \"**/*.{vue,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^1.0.2",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "element-plus": "^2.3.7",
    "pinia": "^2.1.4",
    "qrcode": "^1.5.3",
    "vue": "^3.3.4",
    "vue-hooks-plus": "^1.8.5",
    "vue-router": "^4.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^2.1.0",
    "@types/node": "^18.14.2",
    "@types/qrcode": "^1.5.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/runtime-core": "^3.2.47",
    "electron": "^25.2.0",
    "electron-builder": "^24.4.0",
    "electron-vite": "^1.0.24",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "sass": "^1.58.3",
    "stylelint": "^15.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.2",
    "stylelint-scss": "^4.4.0",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.15.0",
    "unplugin-vue-components": "^0.24.0",
    "vite": "^4.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-optimize-css-modules": "^1.0.4",
    "vite-plugin-stylelint": "^4.2.0",
    "vue-tsc": "^1.0.24"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,vue}": [
      "npm run eslint:fix",
      "npm run stylelint:fix",
      "git add ."
    ],
    "**/*.{scss}": [
      "npm run stylelint:fix",
      "git add ."
    ]
  }
}